    "@types/jsonwebtoken": "^7.2.7",
    "@types/minimist": "^1.2.0",
    "@types/mongodb": "^3.1.1",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^0.4.5",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-google-oauth": "^1.0.38",
